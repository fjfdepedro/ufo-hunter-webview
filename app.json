{
  "expo": {
    "name": "Latest UFO sightings",
    "slug": "ufo.hunters.sightings",
    "privacy": "public",
    "sdkVersion": "34.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/green_ufo.ico",
    "splash": {
      "image": "./assets/splash.png",
